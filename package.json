{
  "name": "chatgpt-clone",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npx nodemon server.js",
    "dev:frontend": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prod": "npm run build && npm run preview",
    "git-update": "git fetch && git pull"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "typescript": "^5.8.2",
    "vite": "^6.2.5"
  }
}
