{
  "name": "chatgpt-clone",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npx nodemon server.js",
    "dev:frontend": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prod": "npm run build && npm run preview",
    "git-update": "git fetch && git pull"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "react": "^18.2.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.67",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.9",
    "typescript": "^5.4.3",
    "vite": "^5.4.14"
  }
}
